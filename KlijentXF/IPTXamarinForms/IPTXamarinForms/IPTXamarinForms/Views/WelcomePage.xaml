<?xml version="1.0" encoding="utf-8" ?>
<ContentPage Title="Welcome to IPT"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IPTXamarinForms.Views.WelcomePage">
    <ContentPage.Content>
        <StackLayout BackgroundColor="#ecf0f1">
            <Button x:Name="btnSettings" Text="Settings" Pressed="btnSettings_Pressed" />
            <Button x:Name="btnClients" Text="Show clients" Pressed="btnClients_Pressed" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>